{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug main.py --test",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/propscrape/main.py",
            "args": ["--test"],
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
            "pythonPath": "${workspaceFolder}/.venv/bin/python.exe",
            "env": {
                "VIRTUAL_ENV": "${workspaceFolder}/.venv",
                "PATH": "${workspaceFolder}/.venv/bin;${env:PATH}"
            }
        }
        ]
}
